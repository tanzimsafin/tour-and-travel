<% layout('layouts/boilerplate') %>
<h1>All Tourmate Posts</h1>
<ul>
    <% posts.forEach(post => { %>
        <li>
            <a href="/tourmatePosts/<%= post._id %>" target="_blank"><%= post.title %></a>
        </li>
    <% }) %>
</ul>
<a href="/tourmatePosts/new" target="_blank">Create New Post</a>